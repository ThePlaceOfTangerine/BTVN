#include <bits/stdc++.h>
using namespace std;

int main()
{
    int n, temp, a, b;
    cin >> n;
    
    a = b = 0;
    for(int i = 0; i < n; ++i){
        cin >> temp;
        a += temp;
    }
    
    for(int i = 0; i <= n; ++i)
    {
        cin >> temp;
        b += temp;
    }
    
    cout << b - a;
    
    return 0;
}
