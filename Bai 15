#include <bits/stdc++.h>
typedef unsigned int ui;
using namespace std;

int camelCase(string s)
{
    int cnt = 0;
    for(ui i = 0; i < s.size(); ++i)
        if(isupper(s[i]) ) ++cnt;
    
    return cnt;
}

int main()
{
    string s;
    cin >> s;
    cout << camelCase(s) + 1;
    
    return 0;
}
